#jinja2: trim_blocks: "true", lstrip_blocks: "true"
{{ ansible_managed | comment }}
# Generated by Ansible role {{ ansible_role_name }}

version: 0.1
log:
  fields:
    service: registry
storage:
  cache:
    blobdescriptor: inmemory
  filesystem:
    rootdirectory: {{ docker_registry_config_storage_fs_root_dir }}
http:
  addr: :{{ docker_registry_config_http_port }}
  headers:
    X-Content-Type-Options: [nosniff]
{% if docker_registry_config_auth_enabled %}
auth:
  htpasswd:
    realm: {{ docker_registry_config_auth_htpasswd_realm }}
    path: /auth/.htpasswd
{% endif %}
proxy:
  remoteurl: {{ docker_registry_config_proxy_remoteurl }}
