version: '3'

services:
  registry:
    image: registry:2.7.0
    restart: always
    volumes:
      - {{ docker_registry_base_dir }}/data:/data
      - {{ docker_registry_base_dir }}/auth:/auth:ro
      - {{ docker_registry_base_dir }}/config.yml:/etc/docker/registry/config.yml:ro
    networks:
      - reg

networks:
  reg:
    external: true
